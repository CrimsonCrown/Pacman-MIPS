 WAITTIME:
 addi $sp,$sp,-4
 sw $ra,0($sp)
 li $a0,5000
 WAITLOOP:
 jal ECHO
 addi $a0,$a0,-1
 bne $a0,$0,WAITLOOP
 lw $ra,0($sp)
 addi $sp,$sp,4
 jr $ra
 
 WAITBIGTIME:
 addi $sp,$sp,-4
 sw $ra,0($sp)
 li $a0,15000
 WAITBIGLOOP:
 jal ECHO
 addi $a0,$a0,-1
 bne $a0,$0,WAITBIGLOOP
 lw $ra,0($sp)
 addi $sp,$sp,4
 jr $ra
 
 INCSCORE:
 addi $sp,$sp,-32
 sw $t0,0($sp)
 sw $t1,4($sp)
 sw $t2,8($sp)
 sw $t3,12($sp)
 sw $t4,16($sp)
 sw $t5,20($sp)
 sw $a1,24($sp)
 sw $ra,28($sp)
 la $t0,SCORE
 sll $t1,$a0,2
 add $t0,$t0,$t1
 lw $t1,0($t0)
 addi $t1,$t1,1
 div $t1,$t1,99999
 mfhi $t2
 sw $t2,0($t0)
 div $t2,$t2,10
 mfhi $t1
 lw $a1,24($sp)
 addi $a1,$a1,80
 jal PRINTCHAR
 div $t2,$t2,10
 mfhi $t1
 lw $a1,24($sp)
 addi $a1,$a1,72
 jal PRINTCHAR
 div $t2,$t2,10
 mfhi $t1
 lw $a1,24($sp)
 addi $a1,$a1,64
 jal PRINTCHAR
 div $t2,$t2,10
 mfhi $t1
 lw $a1,24($sp)
 addi $a1,$a1,56
 jal PRINTCHAR
 div $t2,$t2,10
 mfhi $t1
 lw $a1,24($sp)
 addi $a1,$a1,48
 jal PRINTCHAR
 lw $t0,0($sp)
 lw $t1,4($sp)
 lw $t2,8($sp)
 lw $t3,12($sp)
 lw $t4,16($sp)
 lw $t5,20($sp)
 lw $a1,24($sp)
 lw $ra,28($sp)
 addi $sp,$sp,32
 jr $ra
 
 SCORETABLE:
 addi $sp,$sp,-12
 sw $t0,0($sp)
 sw $t1,4($sp)
 sw $ra,8($sp)
 bne $a0,0,SCORENOTPAC1
 li $a1,0xff000000
 SCORENOTPAC1:
 bne $a0,1,SCORENOTPAC2
 li $a1,0xff000000
 addi $a1,$a1,184
 SCORENOTPAC2:
 bne $a0,2,SCORENOTPAC3
 li $a1,0xff000000
 addi $a1,$a1,74240
 SCORENOTPAC3:
 bne $a0,3,SCORENOTPAC4
 li $a1,0xff000000
 addi $a1,$a1,184
 addi $a1,$a1,74240
 SCORENOTPAC4:
 lw $t0,0($sp)
 lw $t1,4($sp)
 lw $ra,8($sp)
 addi $sp,$sp,12
 jr $ra
 
 PRINTSCORE:
 addi $sp,$sp,-12
 sw $t0,0($sp)
 sw $t1,4($sp)
 sw $ra,8($sp)
 la $t0,SCORE
 sll $t1,$a0,2
 add $t0,$t0,$t1
 lw $t1,0($t0)
 addi $t1,$t1,-1
 sw $t1,0($t0)
 jal SCORETABLE
 jal INCSCORE
 lw $t0,0($sp)
 lw $t1,4($sp)
 lw $ra,8($sp)
 addi $sp,$sp,12
 jr $ra



 END_G:
 li $v0,31
 li $a0,100
 li $a1,200
 li $a2,64
 li $a3,120
 syscall
 jal WAITTIME
 syscall
 jal WAITTIME
 li $a0,110
 syscall
 jal WAITTIME
 li $a0,120
 syscall
 jal WAITTIME
 li $a1,2000
 syscall
 j TRACKDONE
 END:
 li $v0,31
 li $a0,70
 li $a1,200
 li $a2,51
 li $a3,120
 syscall
 jal WAITTIME
 li $a0,66
 syscall
 jal WAITTIME
 li $a0,62
 syscall
 jal WAITTIME
 li $a0,58
 syscall
 jal WAITTIME
 li $a0,55
 li $a1,2000
 syscall
 TRACKDONE:
 jal WAITBIGTIME
 jal WAITBIGTIME
 la $t0, INICIO
 la $t1, AUX
 li $t2, 0
 LOOPYLOOP:
 lw $t3,0($t1)
 sw $t3,0($t0)
 addi $t0,$t0,4
 addi $t1,$t1,4
 addi $t2,$t2,4
 bne $t2,1200,LOOPYLOOP
 la $t9,HP
 lb $t8,0($t9)
 blt $s2,2,HEALTHSUMDONE
 lb $t7,1($t9)
 add $t8,$t8,$t7
 blt $s2,3,HEALTHSUMDONE
 lb $t7,2($t9)
 add $t8,$t8,$t7
 blt $s2,4,HEALTHSUMDONE
 lb $t7,3($t9)
 add $t8,$t8,$t7
 HEALTHSUMDONE:
bne $t8,0,REINC
li $v0, 10
syscall

ECHO:
	la $t1,0xFF100000
	lw $t0,0($t1)
	andi $t0,$t0,0x0001		# Le bit de Controle Teclado
   	beq $t0,$zero,PULA
   	lw $t2,4($t1)  # Tecla lida
   	la $t3,PACBUFFERMOVES
   	beq $t2,119,VNIW
   	beq $t2,100,VNID
   	beq $t2,97,VNIA
   	beq $t2,115,VNIS
   	beq $t2,105,VNII
   	beq $t2,106,VNIJ
   	beq $t2,107,VNIK
   	beq $t2,108,VNIL
   	beq $t2,102,VNIF
   	beq $t2,120,VNIX
   	beq $t2,99,VNIC
   	beq $t2,118,VNIV
   	beq $t2,104,VNIH
   	beq $t2,98,VNIB
   	beq $t2,110,VNIN
   	beq $t2,109,VNIM
   	beq $t2,65,VNIA
   	beq $t2,68,VNID
   	beq $t2,83,VNIS
   	beq $t2,87,VNIW
   	beq $t2,73,VNII
   	beq $t2,74,VNIJ
   	beq $t2,75,VNIK
   	beq $t2,76,VNIL
   	beq $t2,70,VNIF
   	beq $t2,88,VNIX
   	beq $t2,67,VNIC
   	beq $t2,86,VNIV
   	beq $t2,72,VNIH
   	beq $t2,66,VNIB
   	beq $t2,78,VNIN
   	beq $t2,77,VNIM
   	j PULA
   	VNIW:
	add $t1,$t2,$zero
	sb $t1,0($t3)
	j PULA
   	VNIS:
	add $t1,$t2,$zero
	sb $t1,0($t3)
	j PULA
   	VNID:
	add $t1,$t2,$zero
	sb $t1,0($t3)
	j PULA
   	VNIA:
	add $t1,$t2,$zero
	sb $t1,0($t3)
	j PULA
	VNII:
	add $t1,$t2,$zero
	li $t1,87
	sb $t1,1($t3)
	j PULA
	VNIJ:
	add $t1,$t2,$zero
	li $t1,65
	sb $t1,1($t3)
	j PULA
	VNIK:
	add $t1,$t2,$zero
	li $t1,83
	sb $t1,1($t3)
	j PULA
	VNIL:
	add $t1,$t2,$zero
	li $t1,68
	sb $t1,1($t3)
	j PULA
	VNIF:
	add $t1,$t2,$zero
	li $t1,87
	sb $t1,2($t3)
	j PULA
	VNIX:
	add $t1,$t2,$zero
	li $t1,65
	sb $t1,2($t3)
	j PULA
	VNIC:
	add $t1,$t2,$zero
	li $t1,83
	sb $t1,2($t3)
	j PULA
	VNIV:
	add $t1,$t2,$zero
	li $t1,68
	sb $t1,2($t3)
	j PULA
	VNIH:

 bne $t1,1,FN1
 jal PRINT1
 j FGO
 FN1:
 bne $t1,2,FN2
 jal PRINT2
 j FGO
 FN2:
 bne $t1,3,FN3
 jal PRINT3
 j FGO
 FN3:

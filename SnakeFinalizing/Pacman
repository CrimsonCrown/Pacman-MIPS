 la $t1,PACADRESS
 KILLLOOP:
 lw $t2,0($t1)
 bne $t2,$a1,DOKILLLOOP
 li $t2,0
 sw $t2,0($t1)
 la $t1,HP
 add $t1,$t1,$t0
 lb $t2,0($t1)
 addi $t2,$t2,-1
 sb $t2,0($t1)
 la $t1,ISDEAD
 add $t1,$t1,$t0
 li $t2,1
 sb $t2,0($t1)
 jr $ra
 DOKILLLOOP:
 addi $t0,$t0,1
 addi $t1,$t1,4
 j KILLLOOP
 

 .text
# Leitura do teclado e echo na tela
#la $t1,0xFF100000
#li $t0,2
#sw $t0,0($t1)   # Habilita interrup��o do teclado

 la $a1, 0xff000000 
 li $s1, 30
LOOPAOMENU:
 li $s0, 40
LOOPMENU:
 jal PRINTWALL
 addi $a1, $a1, 8
 addi $s0, $s0, -1
 bne $s0, 0, LOOPMENU
 addi $a1, $a1, 2240
 addi $s1, $s1, -1
 bne $s1, 0, LOOPAOMENU
 li $v0,32
 li $a0,5000
 j PRINTSNAKEMENULOGO
 
PRINTLINHA:
 add $t0, $a0, $zero
 add $t0, $t0, $a2
 sub $t1, $a3, $a2
LOOPLINHA:
 sb $a1, 0($t0)
 addi $t1, $t1, -1
 addi $t0, $t0, 1
 bne $t1, 0, LOOPLINHA
 jr $ra
 
PRINTSNAKEMENULOGO:
 la $a0, 0xff000000
 addi $t0, $t0, 320
 mul $t0, $t0, 16
 add $a0, $a0, $t0
 li $a1, 50
 li $a2, 23
 li $a3, 38
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 22
 li $a3, 40
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 18
 li $a3, 42
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 17
 li $a3, 43
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 16
 li $a3, 44
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 16
 li $a3, 45
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 15
 li $a3, 45
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 15
 li $a3, 30
 jal PRINTLINHA
 li $a2, 32
 li $a3, 45
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 14
 li $a3, 27
 jal PRINTLINHA
 li $a2, 34
 li $a3, 45
 jal PRINTLINHA
 addi $a0, $a0, 320

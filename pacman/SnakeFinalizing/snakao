 li $a3, 23
 jal PRINTLINHA
 li $a2, 35
 li $a3, 44
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 11
 li $a3, 21
 jal PRINTLINHA
 li $a2, 37
 li $a3, 43
 jal PRINTLINHA
 addi $a0, $a0, 320
 li $a2, 11
 li $a3, 20
 jal PRINTLINHA
 #line 29
 
 
 li $v0, 30
 syscall
 add $t0, $a1, $zero
 add $a1, $a0, $zero    #rand
 add $a0, $t0, $zero
 li $v0, 40
 syscall
 add $s5, $zero, $a0 
 REINC:
 li $v1,100
 la $t0, INICIO
 la $a1, 0xff000000
 li $t2, 1200
 addi $s2, $t0, 81	#$s2 cabeca
 addi $s5,$zero,543
 
 PUTAPUTALOOP:  #print tela
 addi $t4, $zero, 40
 LOOP: 
 lb $a0, 0($t0)
 bne $a0,46,NOTGRASS
 addi $sp,$sp,-4
 sw $t4,0($sp)
 jal PRINTGROUND
 lw $t4,0($sp)
 addi $sp,$sp,4
 j GOON
 NOTGRASS:
 bne $a0,0,NOTWALL
 addi $sp,$sp,-4
 sw $t4,0($sp)
 jal PRINTWALL
 lw $t4,0($sp)
 addi $sp,$sp,4
 j GOON
 NOTWALL:
 bne $a0,62,NOTHEAD
 addi $sp,$sp,-4
 sw $t4,0($sp)
 jal PRINTGROUND
 lw $t4,0($sp)
 addi $sp,$sp,4
 jal PRINTCABECADIREITA
 j GOON
 NOTHEAD:
 bne $a0,38,NOTCOIN
 addi $sp,$sp,-4
 sw $t4,0($sp)
 jal PRINTGROUND
 lw $t4,0($sp)
 addi $sp,$sp,4
 jal PRINTCOIN
 j GOON
 NOTCOIN:
 addi $sp,$sp,-4
 sw $t4,0($sp)
 jal PRINTGROUND
 lw $t4,0($sp)
 addi $sp,$sp,4
 GOON:
 addi $t0, $t0, 1
 addi $a1, $a1, 8
 addi $t2, $t2, -1
 addi $t4, $t4, -1
 bne $t4, 0, LOOP
 addi $a1, $a1, 2240
 bne $t2, 0, PUTAPUTALOOP
 
 la $a1 0xff000000
 la $t0 HP
 lb $t0,0($t0)
 HEARTS:
 jal PRINTHEART
 addi $a1,$a1,10
 addi $t0,$t0,-1
 bne $t0,0,HEARTS
 jal PRINTSCORE
   
 j JOGO
 
 QUADRADINHO_DE_OITO:
 add $t1, $zero, $a1
 addi $t8, $zero, 8
 QDO_LOOP: 
 sb $a0, 0($t1)
 sb $a0, 1($t1)
 sb $a0, 2($t1)
 sb $a0, 3($t1)
 sb $a0, 4($t1)
 sb $a0, 5($t1)
 sb $a0, 6($t1)
 sb $a0, 7($t1)
 addi $t8, $t8, -1
 addi $t1, $t1, 320
 bne $t8, 0, QDO_LOOP
 jr $ra
 
 JOGO:
 li $a3, 0
 li $s6, 987
 addi $s4, $sp, -4 
 addi $sp, $sp, -12
 li $t4, 68
 sw $t4, 8($sp)
 sw $t4, 4($sp)
 addi $sp,$sp,-20
 sw $a0,0($sp)
 sw $a1,4($sp)
 sw $a2,8($sp)
 sw $a3,12($sp)
 sw $v0,16($sp)
 li $v0,31
 li $a0,70
 li $a1,500
 li $a2,4
 li $a3,40
 syscall    #sound eff
 li $v0,32
 li $a0,1000
 syscall
 li $v0,31
 li $a0,70
 syscall
 li $v0,32
 li $a0,1000
 syscall
 li $v0,31
 li $a0,70
 syscall
 li $v0,32
 li $a0,1000
 syscall
 li $v0,31
 li $a0,80
 li $a1,1000
 syscall
 la $a0,0xFF100000   #set defaul d
 li $a1,100
 sw $a1,4($a0)
 lw $a0,0($sp)
 lw $a1,4($sp)
 lw $a2,8($sp)
 lw $a3,12($sp)
 lw $v0,16($sp)
 addi $sp,$sp,20
 LOOP_JOGO:
 jal WAITTIME
 jal ECHO       #get input
 add $v0,$v1,$zero
 add $s7,$v0,$zero
 
 beq $v0, 65, C_A
 beq $v0, 97, C_A
  
 beq $v0, 83, C_S
 beq $v0, 115, C_S
 
 beq $v0, 68, C_D
 beq $v0, 100, C_D	
  
 beq $v0, 87, C_W
 beq $v0, 119, C_W
 j EXC_CAB
 
 C_A:
 add $t0, $s2, $zero
 lb $t0, -1($t0)
 beq $t0, 0, LOOP_JOGO
 beq $t0, 61, END
 addi $sp,$sp,-20
 sw $a0,0($sp)
 sw $a1,4($sp)
 sw $a2,8($sp)
 sw $a3,12($sp)
 sw $v0,16($sp)
 li $v0,31
 li $a0,90
 li $a1,500
 li $a2,0
 li $a3,40
 syscall
 lw $a0,0($sp)
 lw $a1,4($sp)
 lw $a2,8($sp)
 lw $a3,12($sp)
 lw $v0,16($sp)
 addi $sp,$sp,20
 bne $t0,38,EXC_CAB
 jal INCSCORE   #found rat
 addi $s5,$s5,-1
 beqz $s5,END_G
 j EXC_CAB
 
 C_S:
 add $t0, $zero, $s2
 lb $t0, 40($t0)
 beq $t0, 0, LOOP_JOGO
 beq $t0, 61, END
 addi $sp,$sp,-20
 sw $a0,0($sp)
 sw $a1,4($sp)
 sw $a2,8($sp)
 sw $a3,12($sp)
 sw $v0,16($sp)
 li $v0,31
 li $a0,90
 li $a1,500
 li $a2,0
 li $a3,40
 syscall
 lw $a0,0($sp)
 lw $a1,4($sp)
 lw $a2,8($sp)
 lw $a3,12($sp)
 lw $v0,16($sp)
 addi $sp,$sp,20
 bne $t0,38,EXC_CAB
 jal INCSCORE  #found rat
